[paths]
source = src

[run]
branch = true
source =
    src
    tests
parallel = true
data_file = .cache/coverage
omit =
    .venv/*
    */tests/*
    */__main__.py


[report]
show_missing = true
precision = 2
omit = *migrations*
exclude_lines =
    pragma: no cover
    raise NotImplementedError
    except DistributionNotFound
    TYPE_CHECKING